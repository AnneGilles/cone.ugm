<properties>
  <uri>ldap://127.0.0.1:12345</uri>
  <user>cn=Manager,dc=my-domain,dc=com</user>
  <password>secret</password>
  <users_dn>ou=users,ou=groupOfNames_100_100,dc=my-domain,dc=com</users_dn>
  <users_scope>1</users_scope>
  <users_query></users_query>
  <users_object_classes>
    <item>top</item>
    <item>person</item>
    <item>organizationalPerson</item>
    <item>inetOrgPerson</item>
  </users_object_classes>
  <users_attrmap>
    <elem>
      <key>rdn</key>
      <value>uid</value>
    </elem>
    <elem>
      <key>id</key>
      <value>uid</value>
    </elem>
    <elem>
      <key>login</key>
      <value>uid</value>
    </elem>
  </users_attrmap>
  <users_form_attrmap>
    <elem>
      <key>id</key>
      <value>Id</value>
    </elem>
    <elem>
      <key>userPassword</key>
      <value>Password</value>
    </elem>
    <elem>
      <key>cn</key>
      <value>Fullname</value>
    </elem>
    <elem>
      <key>sn</key>
      <value>Surname</value>
    </elem>
    <elem>
      <key>mail</key>
      <value>Email</value>
    </elem>
  </users_form_attrmap>
  <users_listing_columns>
    <elem>
      <key>name</key>
      <value>cn</value>
    </elem>
    <elem>
      <key>surname</key>
      <value>sn</value>
    </elem>
    <elem>
      <key>email</key>
      <value>email</value>
    </elem>
  </users_listing_columns>
  <users_listing_default_column>name</users_listing_default_column>
  <groups_dn>ou=groups,ou=groupOfNames_100_100,dc=my-domain,dc=com</groups_dn>
  <groups_scope>1</groups_scope>
  <groups_query></groups_query>
  <groups_object_classes>
    <item>groupOfNames</item>
  </groups_object_classes>
  <groups_relation></groups_relation>
  <groups_listing_columns>
    <elem>
      <key>name</key>
      <value>cn</value>
    </elem>
  </groups_listing_columns>
</properties>
